<idea-plugin>
    <id>de.tum.www1.artemis.plugin.intellij.artemis-plugin</id>
    <name>ArTEMiS Programming Exercise Integration</name>
    <vendor email="alexander.ungar@tum.de" url="http://www1.in.tum.de">Technical University of Munich</vendor>

    <depends>com.intellij.modules.vcs</depends>
    <depends>Git4Idea</depends>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="ArTEMiS" anchor="right" factoryClass="de.tum.www1.artemis.plugin.intellij.ui.browser.BrowserFactory" />
        <applicationService serviceInterface="de.tum.www1.artemis.plugin.intellij.vcs.CredentialsService"
                            serviceImplementation="de.tum.www1.artemis.plugin.intellij.vcs.impl.CredentialsServiceImpl"/>
        <projectService serviceImplementation="de.tum.www1.artemis.plugin.intellij.ui.ArtemisRouterService"/>
        <projectService serviceInterface="de.tum.www1.artemis.plugin.intellij.util.ArtemisExerciseRegistry"
                        serviceImplementation="de.tum.www1.artemis.plugin.intellij.util.impl.ArtemisExerciseRegistryImpl"/>
        <projectService serviceInterface="de.tum.www1.artemis.plugin.intellij.bridge.ArtemisBridge"
                        serviceImplementation="de.tum.www1.artemis.plugin.intellij.bridge.ArtemisJSBridge"/>
        <postStartupActivity implementation="de.tum.www1.artemis.plugin.intellij.vcs.ArtemiStartupProjectRefreshActivity"/>
    </extensions>

    <actions>
    </actions>
</idea-plugin>